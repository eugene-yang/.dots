tmux_conf_theme_message_bg='#894a0b'
tmux_conf_theme_window_status_current_bg='#894a0b'      # light blue
tmux_conf_theme_status_left=' [REMOTE]#S '
tmux_conf_theme_status_right_bg='#080808,#894a0b,#e4e4e4' # dark gray, red, white

# replace C-b by C-a instead of using both prefixes
# set -gu prefix2
# unbind C-a
unbind C-b
set -g prefix C-n
bind C-n send-prefix

# move status line to top
set -g status-position top


